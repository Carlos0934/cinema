    "version": "0.2.0",
    "configurations": [
        {
            "request": "launch",
            "name": "Launch Program",
            "type": "node",
            "program": "${workspaceFolder}/main.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "C:\\ProgramData\\chocolatey\\lib\\deno\\deno.EXE",
            "runtimeArgs": [
                "run",
                "--unstable",
                "--import-map",
                "./import_map.json",
                "--inspect-wait",
                "--allow-all",
                "${workspaceFolder}/scraping/src/main.ts"
                
            ],
            "attachSimplePort": 9229
        }
    ]
}